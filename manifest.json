{
    "manifest_version": 2,
    "name": "Hello World",
    "version": "1.0",
    "description": "my first chrome extension",
    "icons": {
        "128": "NB.png"
    },
    "browser_action": {
        "default_icon": "ic.png",
        "default_popup": "popup.html"
    },
    "content_scripts": [{
        "matches": [
            "<all_urls>"
        ],
        "js": ["jquery-3.4.1.min.js"]
    }]
}